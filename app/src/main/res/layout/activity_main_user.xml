<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fadingEdge="horizontal|none"
    tools:context=".MainActivityMenu">

    <ImageButton
        android:id="@+id/monumentosButton"
        android:layout_width="95dp"
        android:layout_height="80dp"
        android:layout_marginTop="200dp"
        android:background="#806DB8B5"
        android:contentDescription="@string/nulo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:srcCompat="@drawable/piramides"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/imageButtonEspaña"
        android:layout_width="87dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:background="#806DB8B5"
        android:contentDescription="@string/nulo"
        app:layout_constraintStart_toEndOf="@+id/monumentosButton"
        app:layout_constraintTop_toTopOf="@+id/monumentosButton"
        app:srcCompat="@drawable/espana"
        tools:ignore="ImageContrastCheck" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="353dp"
        android:layout_height="151dp"
        android:background="#F8F8F8"
        android:orientation="horizontal"
        android:padding="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="90dp"
                android:layout_height="match_parent"
                android:background="#00BCD4"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="match_parent"
                    android:layout_height="98dp"
                    android:contentDescription="@string/todoSCA"
                    app:srcCompat="@drawable/astronauta" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/codigo" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#3F51B5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="196dp"
                        android:layout_height="match_parent"
                        android:gravity="center|left"
                        android:text="  ASTRONAUTA"
                        android:textColor="#FFFFFF"
                        android:textSize="16sp"
                        android:textStyle="bold|italic" />

                    <TextView
                        android:id="@+id/textView13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="N.A.S"
                        android:textSize="20sp" />

                </LinearLayout>

                <TextView
                    android:id="@+id/textUser"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="   Nombre:"
                    android:textColor="#F2F4F0"
                    android:textSize="20sp"
                    android:textStyle="bold|italic" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text=" codigo: 123344555555666" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text=" Puntos:"
                        android:textColor="#FFFFFF"
                        android:textSize="20sp"
                        android:textStyle="bold|italic" />

                    <TextView
                        android:id="@+id/textViewPuntos"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/_0"
                        android:textColor="#F6F5F5"
                        android:textSize="24sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView12"
                        android:layout_width="191dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center|left"
                        android:text=" Rango: Comandante"
                        android:textSize="16sp"
                        android:textStyle="italic" />

                    <ImageView
                        android:id="@+id/imageView11"
                        android:layout_width="46dp"
                        android:layout_height="35dp"
                        app:srcCompat="@drawable/chip" />
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>


    </LinearLayout>

    <ImageButton
        android:id="@+id/imageButtonEuropa"
        android:layout_width="87dp"
        android:layout_height="80dp"
        android:layout_marginEnd="16dp"
        android:background="#806DB8B5"
        android:contentDescription="@string/nulo"
        app:layout_constraintEnd_toStartOf="@+id/monumentosButton"
        app:layout_constraintTop_toTopOf="@+id/monumentosButton"
        app:srcCompat="@drawable/europa"
        tools:ignore="ImageContrastCheck" />

    <ImageButton
        android:id="@+id/imageButtonAleatorio"
        android:layout_width="87dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="#806DB8B5"
        android:contentDescription="@string/nulo"
        app:layout_constraintStart_toStartOf="@+id/imageButtonEuropa"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonEuropa"
        app:srcCompat="@drawable/globo" />

    <ImageButton
        android:id="@+id/imageButtonEeuu"
        android:layout_width="87dp"
        android:layout_height="80dp"
        android:background="#806DB8B5"
        android:contentDescription="@string/nulo"
        app:layout_constraintEnd_toEndOf="@+id/imageButtonEspaña"
        app:layout_constraintStart_toStartOf="@+id/imageButtonEspaña"
        app:layout_constraintTop_toTopOf="@+id/imageButtonAleatorio"
        app:srcCompat="@drawable/estatua" />

    <ImageView
        android:id="@+id/radio"
        android:layout_width="135dp"
        android:layout_height="133dp"
        android:layout_marginBottom="36dp"
        android:contentDescription="@string/nulo"
        android:onClick="dialog"
        app:layout_constraintBottom_toTopOf="@+id/monumentosButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/walkie" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/le"
        android:contentDescription="@string/todoSCAVC" />

    <Switch
        android:id="@+id/switchDificultad"
        android:layout_width="53dp"
        android:layout_height="64dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/switchDificultad"
        app:layout_constraintStart_toEndOf="@+id/switchDificultad"
        app:layout_constraintTop_toTopOf="@+id/switchDificultad"
        app:srcCompat="@drawable/led"
        android:contentDescription="@string/todoK" />

    <ImageButton
        android:id="@+id/reinicioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/tod"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/grabacion" />

    <ImageButton
        android:id="@+id/imageButtonUser"
        android:layout_width="87dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="#806DB8B5"
        android:contentDescription="@string/UU"
        app:layout_constraintEnd_toEndOf="@+id/monumentosButton"
        app:layout_constraintStart_toStartOf="@+id/monumentosButton"
        app:layout_constraintTop_toBottomOf="@+id/monumentosButton"
        app:srcCompat="@drawable/hombre"
        tools:ignore="SpeakableTextPresentCheck,DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/imageButtonNotas"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/reinicioButton"
        app:layout_constraintEnd_toStartOf="@+id/reinicioButton"
        app:layout_constraintTop_toTopOf="@+id/reinicioButton"
        app:layout_constraintVertical_bias="0.473"
        app:srcCompat="@drawable/lapiz"
        tools:ignore="SpeakableTextPresentCheck"
        android:contentDescription="@string/todoUU" />

    <ImageView
        android:id="@+id/dedo"
        android:layout_width="63dp"
        android:layout_height="67dp"
        android:layout_marginTop="96dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.287"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:srcCompat="@drawable/dedos"
        android:contentDescription="@string/todoSCAC" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/clip" />

</androidx.constraintlayout.widget.ConstraintLayout>